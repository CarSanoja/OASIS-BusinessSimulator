{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.oasis.somosquanta.com",
    "VITE_APP_NAME": "OASIS Business Simulator",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_NODE_ENV": "production"
  }
}